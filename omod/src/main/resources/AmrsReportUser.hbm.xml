<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="org.openmrs.module.amrsreport.userreport.UserReport" table="amrsreport_user_report">
 <cache usage="read-write"/>
		<id name="id" type="int"  column="report_user_id"  unsaved-value="0">
		<generator class="org.openmrs.api.db.hibernate.NativeIfNotAssignedIdentityGenerator" />
		</id>
		<many-to-one name="amrsReportsUser" class="org.openmrs.User"	column="user_id"  not-null="false" />
        <property name="reporDefinitionUuid" type="java.lang.String" column="report_definition_uuid" length="38"/>
		<property name="uuid" type="java.lang.String" length="38" not-null="true" unique="true" />
</class>
 </hibernate-mapping>