<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.amrsreport.userlocation">

	<class name="UserLocation" table="amrsreport_userlocation">
		<id name="id" type="int" unsaved-value="0" column="amrsreport_userlocation_id">
			<generator class="native"/>
		</id>
		
		<discriminator column="amrsreport_userlocation_id" insert="false"/>

		<many-to-one name="sysUser" class="org.openmrs.User" not-null="true" />
		
		<property name="uuid" type="java.lang.String" length="38" unique="true"/>
		
		<many-to-one name="userLoc" class="org.openmrs.Location" not-null="true" />
	</class>

</hibernate-mapping>